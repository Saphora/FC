          <Artist id="5587" key="08bbfd0d-ce40-4805-b057-a065adb3ab2f" parentID="4178" level="5" creatorID="3" sortOrder="115" createDate="2016-06-11T21:08:21" updateDate="2016-06-29T13:02:28" nodeName="Y&amp;T" urlName="yt" path="-1,2083,2100,3082,4178,5587" isDoc="" nodeType="3083" creatorName="Magnum44" writerName="Saphora" writerID="0" template="0" nodeTypeAlias="Artist">
