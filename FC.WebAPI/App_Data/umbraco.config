          <Artist id="5768" key="85e9d696-ad8a-446a-a8d1-87d522015a8e" parentID="5724" level="5" creatorID="2" sortOrder="11" createDate="2016-06-20T16:20:45" updateDate="2016-06-29T13:02:20" nodeName="Jacob Collier" urlName="jacob-collier" path="-1,2083,2100,3082,5724,5768" isDoc="" nodeType="3083" creatorName="Knurry" writerName="Saphora" writerID="0" template="0" nodeTypeAlias="Artist">
