          <Artist id="5699" key="717e812c-eb4a-4335-badc-741df98800a9" parentID="4178" level="5" creatorID="3" sortOrder="23" createDate="2016-06-14T19:46:48" updateDate="2016-06-29T13:02:23" nodeName="D.A.D." urlName="dad" path="-1,2083,2100,3082,4178,5699" isDoc="" nodeType="3083" creatorName="Magnum44" writerName="Saphora" writerID="0" template="0" nodeTypeAlias="Artist">
