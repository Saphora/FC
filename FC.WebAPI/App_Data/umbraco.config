          <Artist id="5658" key="1bd827d7-fe3c-4a23-89ed-205a9ec86027" parentID="4178" level="5" creatorID="3" sortOrder="68" createDate="2016-06-12T23:32:15" updateDate="2016-06-29T13:02:26" nodeName="Metal Church" urlName="metal-church" path="-1,2083,2100,3082,4178,5658" isDoc="" nodeType="3083" creatorName="Magnum44" writerName="Saphora" writerID="0" template="0" nodeTypeAlias="Artist">
